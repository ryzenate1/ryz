{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "moduleResolution": "bundler",
    "types": [
      "vite/client",
      "astro/client"
    ]
  },
  "include": [
    ".astro/types.d.ts",
    "src",
    "astro.config.ts",
    "env.d.ts"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
